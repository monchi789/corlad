# SERVIDOR 
DEBUG=True
SECRET_KEY="django-insecure-example-secret-key"

# BASE DE DATOS
ENGINE="django.db.backends.postgresql"
DB_NAME="example_db_name"
DB_USER="example_db_user"
DB_PASSWORD="example_db_password"
DB_HOST="example_db_host"
DB_PORT="5432"

# CORS
CORS_ALLOWED_ORIGINS=http://example.com,http://localhost:3000,https://example.com,http://example:5173,https://example:5173
ALLOWED_HOSTS=example.com,www.example.com,localhost,127.0.0.1,example.com

# SERVIDOR DE CORREO
DEFAULT_FROM_EMAIL=example_email@example.com
EMAIL_BACKEND=django.core.mail.backends.smtp.EmailBackend
EMAIL_HOST=smtp.example.com
EMAIL_PORT=587
EMAIL_USE_TLS=True
EMAIL_HOST_USER=example_email@example.com
EMAIL_HOST_PASSWORD="example_password"
